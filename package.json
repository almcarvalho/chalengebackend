{
  "name": "chalengebackend",
  "version": "1.0.0",
  "description": "Chalenge Backend for Spacial Stations",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register src/myapp.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:create": "typeorm migration:create -n",
    "migration:runAll": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/myapp.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/almcarvalho/chalengebackend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/almcarvalho/chalengebackend/issues"
  },
  "homepage": "https://github.com/almcarvalho/chalengebackend#readme",
  "dependencies": {
    "apollo-datasource-rest": "^3.1.1",
    "apollo-server": "^3.1.2",
    "graphql": "^15.5.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.37"
  },
  "devDependencies": {
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
